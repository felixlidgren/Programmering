        <TextBox x:Name="txtBoxSum"  HorizontalAlignment="Left" Margin="310,276,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblNamn" Content="Resultat" HorizontalAlignment="Left" Margin="341,247,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.579,0.664"/>

    </Grid>
